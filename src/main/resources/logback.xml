<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%m%n</pattern>
			<charset>utf8</charset>
		</encoder>
	</appender>

    <root level="INFO">
		<appender-ref ref="CONSOLE" />
	</root>

    <logger name="app" level="INFO"/>
    <logger name="config" level="INFO"/>
    <logger name="machine" level="INFO"/>
    <logger name="entities" level="INFO"/>

</configuration>
